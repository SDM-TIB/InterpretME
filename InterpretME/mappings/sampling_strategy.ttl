@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix intr: <http://interpretme.org/vocab/> .

<SamplingStrategy>
    rml:logicalSource [
        rml:source "interpretme/files/sampling_strategy.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://interpretme.org/entity/{run_id}";
        rr:class intr:SamplingStrategy
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasSampling;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{sampling}"
        ]
    ].
