@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix intr: <http://interpretme.org/vocab/> .

<SHACLValidationResult>
    rml:logicalSource [
        rml:source "interpretme/files/shacl_validation_results.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://interpretme.org/entity/{index}_{run_id}_{SHACL_Schema}";
        rr:class intr:SHACLValidationResult
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasEntity;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{index}_{run_id}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasRun;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{run_id}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasSHACLResult;
        rr:objectMap [
            rml:reference "SHACL_Result";
            rr:datatype xsd:boolean
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasSHACLSchema;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{SHACL_Schema}_{run_id}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasTargetShape;
        rr:objectMap [
            rml:reference "SHACL_Shape";
            rr:termType rr:IRI
        ]
    ].

<SHACLSchema>
    rml:logicalSource [
        rml:source "interpretme/files/shacl_validation_results.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://interpretme.org/entity/{SHACL_Schema}_{run_id}";
        rr:class intr:SHACLSchema
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasName;
        rr:objectMap [
            rml:reference "SHACL_Schema_Name"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasID;
        rr:objectMap [
            rml:reference "SHACL_Schema"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasRun;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{run_id}"
        ]
    ].

<TargetEntitySHACLConstraint>
    rml:logicalSource [
        rml:source "interpretme/files/shacl_validation_results.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://interpretme.org/entity/{index}_{run_id}";
        rr:class intr:TargetEntity
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasSHACLValidationResult;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{index}_{run_id}_{SHACL_Schema}"
        ]
    ].
