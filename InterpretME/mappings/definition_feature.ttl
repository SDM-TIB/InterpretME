@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix intr: <http://interpretme.org/vocab/> .

<FeatureDefinition>
    rml:logicalSource [
        rml:source "interpretme/files/feature_definition.csv";
        rml:referenceFormulation ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://interpretme.org/entity/{run_id}_{features}";
        rr:class intr:FeatureDefinition
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasRun;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{run_id}"
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate intr:hasFeature;
        rr:objectMap [
            rr:template "http://interpretme.org/entity/{features}"
        ]
    ];
   rr:predicateObjectMap [
        rr:predicate intr:hasDefinition;
        rr:objectMap [
            rml:reference "definition"
        ]
    ].
